{
  "private": true,
  "name": "thread-defender",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "dev:game": "vite build --watch",
    "dev:devvit": "devvit playtest YOUR_DEV_SUBREDDIT",
    "dev": "concurrently -p \"[{name}]\" -n \"DEVVIT,GAME\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run dev:devvit\" \"npm run dev:game\" --restart-tries 30",
    "vite": "vite",
    "build": "vite build && devvit build",
    "upload": "devvit upload"
  },
  "dependencies": {
    "@devvit/public-api": "^0.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
